      <body>
         <div xmlns:ecep="http://www.eighteenthcenturypoetry.org/ns"
              ana="#iamb #pentameter #quatrain #cross #epitaph"
              corresp="#txt_GrayTh1716_wepww"
              ecep:syllab="10.10.10.10"
              met="-+|-+|-+|-+|-+/ -+|-+|-+|-+|-+/ -+|-+|-+|-+|-+/ -+|-+|-+|-+|-+/"
              rendition="#lgindent"
              rhyme="abab"
              subtype="eceptext"
              type="poem"
              xml:id="tgaen-wepww"
              decls="#semi-normalized">
            <link target="authors.xml#aut_GrayTh1716" type="author"/>
            <head subtype="supplied" type="main">
               <pc xml:id="tgaen-214690" n="a-0010">[</pc>
               <w xml:id="tgaen-214700" n="a-0020">Epitaph</w>
               <c/>
               <w xml:id="tgaen-214710" n="a-0030">on</w>
               <c/>
               <w xml:id="tgaen-214720" n="a-0040">Sir</w>
               <c/>
               <w xml:id="tgaen-214730" n="a-0050">William</w>
               <c/>
               <w xml:id="tgaen-214740" n="a-0060">Williams</w>
               <pc xml:id="tgaen-214750" n="a-0070" unit="sentence">]</pc>
            </head>
            <lg xml:id="tgaen-s01100">
               <l n="1" xml:id="tgaen-l09925">
                  <w xml:id="tgaen-214780" n="a-0080">Here</w>
                  <pc xml:id="tgaen-214790" n="a-0090">,</pc>
                  <c/>
                  <w xml:id="tgaen-214800" n="a-0100">foremost</w>
                  <c/>
                  <w xml:id="tgaen-214810" n="a-0110">in</w>
                  <c/>
                  <w xml:id="tgaen-214820" n="a-0120">the</w>
                  <c/>
                  <w xml:id="tgaen-214830" n="a-0130">dangerous</w>
                  <c/>
                  <w xml:id="tgaen-214840" n="a-0140">paths</w>
                  <c/>
                  <w xml:id="tgaen-214850" n="a-0150">of</w>
                  <c/>
                  <w xml:id="tgaen-214860" n="a-0160">fame</w>
                  <pc xml:id="tgaen-214870" n="a-0170">,</pc>
               </l>
               <l n="2" xml:id="tgaen-l09930">
                  <w xml:id="tgaen-214880" n="a-0180">Young</w>
                  <c/>
                  <w xml:id="tgaen-214890" n="a-0190">Williams</w>
                  <c/>
                  <w xml:id="tgaen-214900" n="a-0200">fought</w>
                  <c/>
                  <w xml:id="tgaen-214910" n="a-0210">for</w>
                  <c/>
                  <w xml:id="tgaen-214920" n="a-0220">England's</w>
                  <c/>
                  <w xml:id="tgaen-214930" n="a-0230">fair</w>
                  <c/>
                  <w xml:id="tgaen-214940" n="a-0240">renown</w>
                  <pc xml:id="tgaen-214950" n="a-0250">;</pc>
               </l>
               <l n="3" xml:id="tgaen-l09935">
                  <w xml:id="tgaen-214960" n="a-0260">His</w>
                  <c/>
                  <w xml:id="tgaen-214970" n="a-0270">mind</w>
                  <c/>
                  <w xml:id="tgaen-214980" n="a-0280">each</w>
                  <c/>
                  <w xml:id="tgaen-214990" n="a-0290">Muse</w>
                  <pc xml:id="tgaen-215000" n="a-0300">,</pc>
                  <c/>
                  <w xml:id="tgaen-215010" n="a-0310">each</w>
                  <c/>
                  <w xml:id="tgaen-215020" n="a-0320">Grace</w>
                  <c/>
                  <w xml:id="tgaen-215030" n="a-0330">adorned</w>
                  <c/>
                  <w xml:id="tgaen-215040" n="a-0340">his</w>
                  <c/>
                  <w xml:id="tgaen-215050" n="a-0350">frame</w>
                  <pc xml:id="tgaen-215060" n="a-0360">,</pc>
               </l>
               <l n="4" xml:id="tgaen-l09940">
                  <w xml:id="tgaen-215070" n="a-0370">Nor</w>
                  <c/>
                  <w xml:id="tgaen-215080" n="a-0380">Envy</w>
                  <c/>
                  <w xml:id="tgaen-215090" n="a-0390">dared</w>
                  <c/>
                  <w xml:id="tgaen-215100" n="a-0400">to</w>
                  <c/>
                  <w xml:id="tgaen-215110" n="a-0410">view</w>
                  <c/>
                  <w xml:id="tgaen-215120" n="a-0420">him</w>
                  <c/>
                  <w xml:id="tgaen-215130" n="a-0430">with</w>
                  <c/>
                  <w xml:id="tgaen-215140" n="a-0440">a</w>
                  <c/>
                  <w xml:id="tgaen-215150" n="a-0450">frown</w>
                  <pc xml:id="tgaen-215160" n="a-0460" unit="sentence">.</pc>
               </l>
            </lg>
            <lg xml:id="tgaen-s01105">
               <l n="5" xml:id="tgaen-l09945">
                  <w xml:id="tgaen-215190" n="a-0470">At</w>
                  <c/>
                  <w xml:id="tgaen-215200" n="a-0480">Aix</w>
                  <c/>
                  <w xml:id="tgaen-215210" n="a-0490">uncalled</w>
                  <c/>
                  <w xml:id="tgaen-215220" n="a-0500">his</w>
                  <c/>
                  <w xml:id="tgaen-215230" n="a-0510">maiden</w>
                  <c/>
                  <w xml:id="tgaen-215240" n="a-0520">sword</w>
                  <c/>
                  <w xml:id="tgaen-215250" n="a-0530">he</w>
                  <c/>
                  <w xml:id="tgaen-215260" n="a-0540">drew</w>
                  <pc xml:id="tgaen-215270" n="a-0550">,</pc>
               </l>
               <l n="6" xml:id="tgaen-l09950">
                  <pc xml:id="tgaen-215280" n="a-0560">(</pc>
                  <w xml:id="tgaen-215290" n="a-0570">There</w>
                  <c/>
                  <w xml:id="tgaen-215300" n="a-0580">first</w>
                  <c/>
                  <w xml:id="tgaen-215310" n="a-0590">in</w>
                  <c/>
                  <w xml:id="tgaen-215320" n="a-0600">blood</w>
                  <c/>
                  <w xml:id="tgaen-215330" n="a-0610">his</w>
                  <c/>
                  <w xml:id="tgaen-215340" n="a-0620">infant</w>
                  <c/>
                  <w xml:id="tgaen-215350" n="a-0630">glory</w>
                  <c/>
                  <w xml:id="tgaen-215360" n="a-0640">sealed</w>
                  <pc xml:id="tgaen-215370" n="a-0650">)</pc>
                  <pc xml:id="tgaen-215380" n="a-0660">;</pc>
               </l>
               <l n="7" xml:id="tgaen-l09955">
                  <w xml:id="tgaen-215390" n="a-0670">From</w>
                  <c/>
                  <w xml:id="tgaen-215400" n="a-0680">fortune</w>
                  <pc xml:id="tgaen-215410" n="a-0690">,</pc>
                  <c/>
                  <w xml:id="tgaen-215420" n="a-0700">pleasure</w>
                  <pc xml:id="tgaen-215430" n="a-0710">,</pc>
                  <c/>
                  <w xml:id="tgaen-215440" n="a-0720">science</w>
                  <pc xml:id="tgaen-215450" n="a-0730">,</pc>
                  <c/>
                  <w xml:id="tgaen-215460" n="a-0740">love</w>
                  <pc xml:id="tgaen-215470" n="a-0750">,</pc>
                  <c/>
                  <w xml:id="tgaen-215480" n="a-0760">he</w>
                  <c/>
                  <w xml:id="tgaen-215490" n="a-0770">flew</w>
                  <pc xml:id="tgaen-215500" n="a-0780">,</pc>
               </l>
               <l n="8" xml:id="tgaen-l09960">
                  <w xml:id="tgaen-215510" n="a-0790">And</w>
                  <c/>
                  <w xml:id="tgaen-215520" n="a-0800">scorned</w>
                  <c/>
                  <w xml:id="tgaen-215530" n="a-0810">repose</w>
                  <c/>
                  <w xml:id="tgaen-215540" n="a-0820">when</w>
                  <c/>
                  <w xml:id="tgaen-215550" n="a-0830">Britain</w>
                  <c/>
                  <w xml:id="tgaen-215560" n="a-0840">took</w>
                  <c/>
                  <w xml:id="tgaen-215570" n="a-0850">the</w>
                  <c/>
                  <w xml:id="tgaen-215580" n="a-0860">field</w>
                  <pc xml:id="tgaen-215590" n="a-0870" unit="sentence">.</pc>
               </l>
            </lg>
            <lg xml:id="tgaen-s01110">
               <l n="9" xml:id="tgaen-l09965">
                  <w xml:id="tgaen-215620" n="a-0880">With</w>
                  <c/>
                  <w xml:id="tgaen-215630" n="a-0890">eyes</w>
                  <c/>
                  <w xml:id="tgaen-215640" n="a-0900">of</w>
                  <c/>
                  <w xml:id="tgaen-215650" n="a-0910">flame</w>
                  <c/>
                  <w xml:id="tgaen-215660" n="a-0920">and</w>
                  <c/>
                  <w xml:id="tgaen-215670" n="a-0930">cool</w>
                  <c/>
                  <w xml:id="tgaen-215680" n="a-0940">intrepid</w>
                  <c/>
                  <w xml:id="tgaen-215690" n="a-0950">breast</w>
                  <pc xml:id="tgaen-215700" n="a-0960">,</pc>
               </l>
               <l n="10" xml:id="tgaen-l09970">
                  <w xml:id="tgaen-215710" n="a-0970">Victor</w>
                  <c/>
                  <w xml:id="tgaen-215720" n="a-0980">he</w>
                  <c/>
                  <w xml:id="tgaen-215730" n="a-0990">stood</w>
                  <c/>
                  <w xml:id="tgaen-215740" n="a-1000">on</w>
                  <c/>
                  <w xml:id="tgaen-215750" n="a-1010">Belle</w>
                  <c/>
                  <w xml:id="tgaen-215760" n="a-1020">Isle's</w>
                  <c/>
                  <w xml:id="tgaen-215770" n="a-1030">rocky</w>
                  <c/>
                  <w xml:id="tgaen-215780" n="a-1040">steeps</w>
                  <pc xml:id="tgaen-215790" n="a-1050">;</pc>
               </l>
               <l n="11" xml:id="tgaen-l09975">
                  <w xml:id="tgaen-215800" n="a-0010">Ah</w>
                  <c/>
                  <w xml:id="tgaen-215810" n="a-0020">gallant</w>
                  <c/>
                  <w xml:id="tgaen-215820" n="a-0030">youth</w>
                  <pc xml:id="tgaen-215830" n="a-0040">!</pc>
                  <c/>
                  <w xml:id="tgaen-215840" n="a-0050">this</w>
                  <c/>
                  <w xml:id="tgaen-215850" n="a-0060">marble</w>
                  <c/>
                  <w xml:id="tgaen-215860" n="a-0070">tells</w>
                  <c/>
                  <w xml:id="tgaen-215870" n="a-0080">the</w>
                  <c/>
                  <w xml:id="tgaen-215880" n="a-0090">rest</w>
                  <pc xml:id="tgaen-215890" n="a-0100">,</pc>
               </l>
               <l n="12" xml:id="tgaen-l09980">
                  <w xml:id="tgaen-215900" n="a-0110">Where</w>
                  <c/>
                  <w xml:id="tgaen-215910" n="a-0120">melancholy</w>
                  <c/>
                  <w xml:id="tgaen-215920" n="a-0130">Friendship</w>
                  <c/>
                  <w xml:id="tgaen-215930" n="a-0140">bends</w>
                  <c/>
                  <w xml:id="tgaen-215940" n="a-0150">and</w>
                  <c/>
                  <w xml:id="tgaen-215950" n="a-0160">weeps</w>
                  <pc xml:id="tgaen-215960" n="a-0170" unit="sentence">.</pc>
               </l>
            </lg>
         </div>
      </body>
