                  <unittitle type="uniform" label="Uniform Title:">
                     <title render="doublequote" linktype="simple">On L[or]d H[olland']s Seat near M[argat]e,
								K[en]t</title>
                  </unittitle>
                  <unittitle label="First Line:">
                     <title linktype="simple">Old and abandoned by each venal friend,</title>
                  </unittitle>
                  <unitdate normal="1769" era="ce" calendar="gregorian">1769</unitdate>
