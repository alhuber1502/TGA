                  <unittitle type="uniform" label="Uniform Title:">
                     <title render="doublequote">William Shakespeare to Mrs Anne, Regular
								Servant to the Revd Mr Precentor of York</title>
                  </unittitle>
                  <unittitle label="First Line:">
                     <title>A moment's patience, gentle Mistress Anne!</title>
                  </unittitle>
                  <unitdate normal="1853">1853</unitdate>
               </did>
               <bibliography>
                  <bibref title="https://www.thomasgray.org/resources/biblp.shtml#S/H_1966">Starr/Hendrickson (eds.), <title render="italic">Complete Poems</title> (1966),  80-81</bibref>
