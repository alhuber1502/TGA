                  <unitid type="work">poems.sahh</unitid>
                  <unittitle type="uniform" label="Uniform Title:">
                     <title render="doublequote">Satire on the Heads of Houses; or, Never a
								Barrel the Better Herring</title>
                  </unittitle>
                  <unittitle label="First Line:">
                     <title>O Cambridge, attend</title>
                  </unittitle>
                  <unitdate normal="1884">1884</unitdate>
               </did>
               <bibliography>
                  <bibref title="https://www.thomasgray.org/resources/biblp.shtml#S/H_1966">Starr/Hendrickson (eds.), <title render="italic">Complete Poems</title> (1966),  76-77</bibref>
