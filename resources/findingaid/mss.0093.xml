                  <unittitle label="First Line:">
                     <title linktype="simple">Pendet Homo incertus gemini ad confinia mundi</title>
                  </unittitle>
                  <unitdate normal="1882/1884" era="ce" calendar="gregorian">1882 and 1884</unitdate>
