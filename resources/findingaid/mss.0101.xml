            <c02 level="class">
               <did>
                  <unitid type="work">poems.ocmu</unitid>
                  <unittitle type="uniform" label="Uniform Title:">
                     <title render="doublequote">Ode for Music</title>
                  </unittitle>
                  <unittitle label="First Line:">
                     <title>"Hence, avaunt, ('tis holy ground)</title>
                  </unittitle>
                  <unitdate normal="1769">1769</unitdate>
               </did>
               <bibliography>
                  <bibref title="https://www.thomasgray.org/resources/biblp.shtml#S/H_1966">Starr/Hendrickson (eds.), <title render="italic">Complete Poems</title> (1966),  48-51</bibref>
